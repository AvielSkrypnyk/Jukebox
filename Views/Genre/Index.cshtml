@model IEnumerable<MPAJukebox.Models.Genre>

<div class="container mt-4">
    <div class="row">
        @foreach (var genre in Model)
        {
        <div class="col-md-3 mb-4">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">@genre.Name</h5>
                </div>
            </div>
        </div>
        }
    </div>
</div>